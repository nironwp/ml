                <span style="margin:5px 10px 20px;display:block;font-family:proximaNovaRegular;font-size:20px;color:rgba(0,0,0,.9);">Recomendado para você</span>
                <div class="carrossel2">
                    <% produtos_aleatorios.forEach(produto => { 
                        const desconto = ((produto.preco_original - produto.preco_atual) / produto.preco_original * 100).toFixed(0); // Mantém duas casas decimais
                     %>
                       <div onclick="acessarLink('/produtos/<%= produto.id %>');" style="margin-left:10px;margin-right:10px;min-width:220px;width:220px;max-width:220px;background-color:#ffffff;border-radius:4px;">
                         <div style="flex-grow:1;height:220px;position:relative;background-image:url('<%= produto.imagem_capa %>');background-size:contain;background-repeat:no-repeat;background-position:center;"></div>
                         <div style="margin:15px;">
                           <span style="font-family:proximaNovaRegular;font-size:12px;color:rgba(0,0,0,.55);text-decoration:line-through;">R$ <%= produto.preco_original.toFixed(2) %></span>
                           <div style="width:max-content;display:flex;">
                             <span style="font-family:proximaNovaLight;font-size:24px;color:#333333;">R$ <%= produto.preco_atual.toFixed(2) %></span>
                             <span style="margin:5px 0 0 5px;font-family:proximaNovaRegular;font-size:14px;color:#00a650;"><%= desconto %>% OFF</span>
                           </div>
                           <div style="margin-top:10px;flex-grow:1;height:30px;word-break:break-word;text-align:left;display:flex;align-items:center;">
                             <span style="font-family:proximaNovaRegular;font-size:14px;color:rgba(0,0,0,.8);"><%= produto.nome_produto %></span>
                           </div>
                         </div>
                       </div>
                     <% }); %>                     
                </div>
            </div>
        </main>
